{
  "name": "vue2-event-calendar",
  "version": "0.4.3",
  "description": "A vue2 calendar component, have week and month two mode, custom render event",
  "main": "vue2-event-calendar.js",
  "scripts": {
    "serve": "vue-cli-service serve dev/index.js",
    "build": "rvue-cli-service build --target lib --name vue2-event-calendar src/index.vue",
    "deploy": "npm run test && node build/build && npm run build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test": "npm run lint && npm run test:unit",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "vue",
    "calendar"
  ],
  "typings": "./src/index.d.ts",
  "author": "kitwang chan",
  "license": "MIT",
  "repository": "https://github.com/kitwon/vue2-calendar",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/addons": "^5.0.6",
    "@storybook/vue": "^5.0.6",
    "@types/jest": "^23.3.14",
    "@types/storybook__vue": "^5.0.1",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0-beta.3",
    "@vue/babel-preset-jsx": "^1.0.0-beta.3",
    "@vue/cli-plugin-babel": "^3.5.5",
    "@vue/cli-plugin-typescript": "^3.5.3",
    "@vue/cli-plugin-unit-jest": "^3.5.3",
    "@vue/cli-service": "^3.5.3",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "^8.0.5",
    "babel-preset-vue": "^2.0.2",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "ts-jest": "^23.0.0",
    "typescript": "^3.4.2",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "dayjs": "^1.8.12",
    "vue": "^2.6.10",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.3.0"
  }
